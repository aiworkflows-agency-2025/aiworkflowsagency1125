{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*.{js,css,png,jpg,gif,ico}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache, no-store, must-revalidate"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
